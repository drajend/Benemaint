
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    body {
        font-size: 1.2em;
    }

    div.dataTables_wrapper {
        width: 400px;
        margin: 0 0;
    }

    #dataPolicy3_wrapper {width: 300px;}
    #dataPolicy3_paginate{display:none;}
    #dataPolicy3_info{display:none;}
   #parent{padding: 5% 0;}
   #child {padding: 50%;}
   .countTB {width: 15px; height: 20px}

</style>

<div class="panel-body" style="padding-right: 2px; padding-left: 2px; padding-top:1px; padding-bottom:2px">
 
    <div id="resultSearch" hidden>
        
        <div id="panel-body nopadding">
            
            <div>
                <h4 class="hrCustom">Participant/Account Info</h4>
                <hr class="hrwise" />
                <div class="input-group input-group-sm form-inline panel nomarginbottom">

                    <span class="input-group-addon" style="width: 14.4%; line-height: 0px !important; font-size: 11px;">Name</span>
                    <input class="form-control" id="fldName" style="font-size: 11px; width: 99.9%; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="50">
                    <span class="input-group-addon" style="width: 14.1%; line-height: 0px !important; font-size: 11px;">SSN</span>
                    <input class="form-control" id="fldSSN" style="font-size: 11px; width: 99.5%; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">
                </div>

                <div style="width:100%;text-align:center">
                    <input type="radio" name="applyTo" value="AnnuityAccountsOnly" onclick="javascript:annuityAccounts();" style="margin-left:30px"> Annuity Accounts Only
                    <input type="radio" name="applyTo" value="MutualFundAccountsOnly" onclick="javascript:mutualFundAccount();" style="margin-left:30px"> MutualFund Accounts Only
                    <input type="radio" name="applyTo" value="SpecificAccounts" onclick="javascript:specifiedAccounts();" style="margin-left:30px" checked> Specific Accounts
                </div>

                <div style="float:left;width:40%">
                    <div class="dataTables_wrapper form-inline dt-bootstrap no-footer" id="dataTables-example_wrapper">
                        <div class="row">
                            <div class="col-lg-12">
                                <table class="display" string id="dataPolicy" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Account</th>
                                            <th>Product</th>
                                            <th>GrpNo</th>
                                            <th>Plan</th>
                                            <th>Sub</th>
                                            <th>SSN Type</th>
                                            <th>Account Code</th>
                                            <th>Participant Name</th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div style="float:left;width:10%;align-items:center;vertical-align:middle">
                    <div id="parent">
                        <div id="child">
                            <button id="btnAdd" type="button" data-placement="bottom" data-toggle="tooltip" title="" class="btn btn-info btn-sm autoFontSize"> &nbsp;&nbsp; Add &nbsp;&nbsp;&nbsp; </button>
                            <button id="btnRemove" type="button" data-placement="bottom" data-toggle="tooltip" title="" class="btn btn-warning btn-sm autoFontSize"> Remove </button>

                        </div>
                    </div>
                </div>

                <div style="float:right;width:40%;margin-left:-10px">
                    <div class="dataTables_wrapper form-inline dt-bootstrap no-footer" id="dataTables-example_wrapper">
                        <div class="row">
                            <div class="col-lg-12">
                                <table class="display" string id="dataPolicy2" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Account</th>
                                            <th>Product</th>
                                            <th>GrpNo</th>
                                            <th>Plan</th>
                                            <th>Sub</th>
                                            <th>SSN Type</th>
                                            <th>Account Code</th>
                                            <th>Participant Name</th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div style="width:100%;border:1px solid black; display:inline-block ">
                <div style="width:30%;float:left; ">
                   
                    <div>
                        <h4 class="hrCustom">Percent Totals</h4>
                        <hr class="hrwise" />
