                                <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">DOB</span>
                                <input class="form-control" id="fldPDOB" style="font-size: 11px; width: 110px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">
                                
                                <span class="input-group-addon" style="width: 80px; min-width:100px;line-height: 0px !important; font-size: 11px;">Address Line 2</span>
                                <input class="form-control" id="fldPAddress2" style="font-size: 11px; width: 260px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">
                            </div>
                            <div class="input-group input-group-sm form-inline panel nomarginbottom">

                               <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">Relationship</span>
                                @*<input class="form-control" id="fldPRelationship" style="font-size: 11px; width: 99.5%; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">*@
                                <select class="form-control" id="fldPRelationship" style="font-size: 11px; width: 120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="50">
                                    <option value="SPOUSE">SPOUSE</option>
                                    <option value="NON-SPOUSE">NON-SPOUSE</option>
                                    <option value="TRUST">TRUST</option>
                                    <option value="OTHERS">OTHERS</option>
                                </select>
                                <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">City</span>
                                <input class="form-control" id="fldPCity" style="font-size: 11px; width: 120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="50">
                                <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">State</span>
                                <input class="form-control" id="fldPState" style="font-size: 11px; width: 120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">

                               
                            </div>
                            @*<div class="input-group input-group-sm form-inline panel nomarginbottom">
                              
                            </div>*@
                            <div class="input-group input-group-sm form-inline panel nomarginbottom">
                                
                                <span class="input-group-addon" style="width: 80px; min-width:82px; line-height: 0px !important; font-size: 11px;">Percent</span>
                                <input class="form-control" id="fldPPercentage" style="font-size: 11px; width: 120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="50">

                                <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">Zip</span>
                                <input class="form-control" id="fldPZip" style="font-size: 11px; width: 120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="50">
                                <span class="input-group-addon" style="width: 80px; min-width:80px; line-height: 0px !important; font-size: 11px;">Foreign Zip</span>
                                <input class="form-control" id="fldPForeignZip" style="font-size: 11px; width:120px; padding-top: 0px !important; padding-bottom: 0px !important;" type="text" maxlength="35">


                            </div>
                            @*<div class="input-group input-group-sm form-inline panel nomarginbottom">
                                 <span class="input-group-addon" style="width: 50%; line-height: 0px !important; font-size: 11px;"></span>
                            </div>*@

                        </div>


