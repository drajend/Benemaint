                            <div class="row">
                                <div class="col-lg-12">
                                    <table class="display" string id="dataPolicy3" cellspacing="0">
                                        <thead>
                                            <tr>
                                                <th>Sort</th>
                                                <th>Primary</th>
                                                <th>Contingent</th>
                                            </tr>
                                        </thead>
                                    </table>
                                    <div id="divTotal"><span id="spTotal" style="font-weight:bold;"></span></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div style="width:70%;float:right;border-left:1px solid black;">
                    <div class="panel panel-body" style="padding: 0px !important;" data-bind="foreach: Contract">
